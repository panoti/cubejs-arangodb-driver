{
  "name": "arangodbx-cubejs-driver",
  "description": "Cube.js arangodb driver",
  "author": "pnthanh",
  "version": "0.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/panoti/cubejs-arangodb-driver.git"
  },
  "engines": {
    "node": "^12.0.0 || ^14.0.0 || >=16.0.0"
  },
  "main": "dist/ArangoDbDriver.js",
  "typings": "dist/ArangoDbDriver.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "lint": "eslint src/* test/* --ext .ts,.js",
    "lint:fix": "eslint --fix src/* test/* --ext .ts,.js",
    "test": "jest",
    "test:integration": "jest --config ./test/jest-intergration.json"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "arangojs": "^7.8.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@cubejs-backend/linter": "^0.30.0",
    "@cubejs-backend/query-orchestrator": "^0.30.34",
    "@cubejs-backend/schema-compiler": "^0.30.34",
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "rimraf": "^3.0.2",
    "testcontainers": "^8.12.0",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "eslintConfig": {
    "extends": "./node_modules/@cubejs-backend/linter/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
