{
  "extends": "./tsconfig.base.json",
  "include": [
    "src"
  ],
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "./src",
    "baseUrl": ".",
    "allowJs": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "strictBindCallApply": false,
    "strictPropertyInitialization": false,
  },
}